{
  "name": "bccampus/eypd",
  "description": "Theme and functionality to support Early Years Professional Development",
  "type": "wordpress-theme",
  "version": "0.9.6",
  "homepage": "https://github.com/bccampus/eypd",
  "authors": [
    {
      "name": "Brad Payne",
      "email": "brad@bradpayne.ca",
      "homepage": "https://bradpayne.ca"
    },
    {
      "name": "Alex Paredes",
      "email": "aparedes@bccampus.ca"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "humanmade/coding-standards": "^0.2.1"
  },
  "scripts": {
    "test": [
      "@standards"
    ],
    "standards": [
      "vendor/bin/phpcs --standard=phpcs.ruleset.xml *.php ."
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/roots/soil"
    }
  ],
  "extra": {
    "installer-paths": {
      "vendor/roots/soil": ["roots/soil"]
    }
  },
  "require": {
    "roots/soil": "^3.7"
  }
}
